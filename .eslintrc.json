{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "node": true
    },
    "globals": {
        "Vue": true,
        "fetch": true,
        "window": true,
        "$": true,
        "document": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "off",
            4
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-unused-vars": "off",
        "no-console": "off",
        "strict": 0
    }
}